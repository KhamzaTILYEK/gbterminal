{
  "name": "gbterminal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "source .env && react-native start --port $IOS_PORT --reset-cache",
    "ios": "source .env && react-native run-ios --port=$IOS_PORT",
    "ios_release": "source .env && react-native run-ios -configuration Release --port=$IOS_PORT",
    "start2": "react-native start --port 8081 --reset-cache",
    "android": " react-native run-android --port=8081",
    "android_release": "source .env && react-native run-android --mode release --port=$ANDROID_PORT",
    "debug": "source .env && open \"rndebugger://set-debugger-loc?host=localhost&port=$ANDROID_PORT\"",
    "sha": "keytool -list -v -alias android -keystore ./android/android.keystore",
    "test": "jest",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "pod": "NO_FLIPPER=1 bundle exec pod install --repo-update --project-directory=ios",
    "device": "source .env && adb reverse tcp:$ANDROID_PORT tcp:$ANDROID_PORT",
    "man": "watchman watch-del ${PWD} ; watchman watch-project ${PWD}",
    "refresh": "yarn cache clean && rm -rf node_modules && cd ios && pod deintegrate && pod cache clean --all && cd .. && rm -rf android/build && yarn install && yarn man",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "upload_words": "source .env && curl -X POST -H \"Content-Type: application/json\" -d @./src/locales/$LANGUAGE.json $LANG_HOST/api/locales/add/$LANGUAGE/common?auth_key=$AUTH_KEY",
    "download_words": "source .env && curl $LANG_HOST/api/locales/$LANGUAGE/common.json?auth_key=$AUTH_KEY --output src/locales/$LANGUAGE.json"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "^1.19.6",
    "@react-native-community/cli-doctor": "^13.2.0",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "deprecated-react-native-prop-types": "^5.0.0",
    "i18n-js": "^4.3.2",
    "libphonenumber-js": "^1.10.49",
    "lodash": "^4.17.21",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^3.0.3",
    "react-native-paper": "^5.11.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^13.14.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-virtualized-view": "^1.0.0",
    "react-native-vision-camera": "^3.6.17",
    "react-native-worklets-core": "^0.2.4",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0"
  },
  "engines": {
    "node": ">=16"
  }
}
